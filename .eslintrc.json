{
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": [
      2,
      "single"
    ],
    "strict": [
      2,
      "never"
    ],
    "indent": 2,
    "eqeqeq": [
      "error",
      "always"
    ],
    "no-multi-spaces": "error",
    "keyword-spacing": "error",
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/jsx-no-undef": 2,
    "react/react-in-jsx-scope": 2
  },
  "plugins": [
    "react"
  ]
}